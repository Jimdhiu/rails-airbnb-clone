<h1>List all my bookings</h1>


<div class="container">
  <div class="row">
    <% @bookings.each do |booking| %>
      <div class="col-xs-12 col-sm-4">
        <%= render "boats/boat", boat: booking.boat %>

        <%= link_to  "Consult", booking_path(booking)  %>
        <%= link_to "Delete", booking_path(booking), method: :delete %>
      </div>
    <% end %>
  </div>
</div>
